{
  "name": "totor13x/laravel-pubsub",
  "description": "A Pub-Sub abstraction for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Superbalist.com a division of Takealot Online (Pty) Ltd",
      "email": "info@superbalist.com"
    }
  ],
  "conflict": {
    "superbalist/laravel-pubsub": "*"
  },
  "require": {
    "php": ">=8.1.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/console": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4.2"
  },
  "autoload": {
    "psr-4": {
      "LeroyMerlin\\LaravelPubSub\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
